{
  "name": "react-starter-kit",
  "version": "0.0.1",
  "description": "React webpack starter kit",
  "license": "MIT",
  "keywords": [
    "webpack",
    "react",
    "webpack"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-serve --config ./webpack.config.js --port 3000 --open",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "format": "prettier --write '**/*.{tsx,ts,css,md,js}'"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.1.0",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "prettier": "^1.13.7",
    "prop-types": "^15.6.2",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-serve": "^2.0.2"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "styled-components": "^3.3.3"
  }
}
